<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent" android:layout_height="match_parent"
        tools:ignore="MissingDefaultResource">

    <ImageView
            android:layout_width="@dimen/image_size_horizontal"
            android:layout_height="@dimen/image_size_horizontal"
            app:srcCompat="@drawable/email"
            android:id="@+id/imageView12"
            app:layout_constraintStart_toStartOf="@+id/imageView11"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView11"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/name"
            app:fontFamily="@font/fonttittle"
            android:textSize="34sp"
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/fullname"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"/>

    <TextView
            android:text="@string/telegram"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            app:fontFamily="@font/contentfont"
            app:layout_constraintTop_toTopOf="@+id/imageView11"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView11"
            app:layout_constraintStart_toEndOf="@+id/imageView11"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"/>

    <TextView
            android:text="@string/location"
            android:layout_width="147dp"
            android:layout_height="27dp"
            android:id="@+id/location"
            app:fontFamily="@font/contentfont"
            android:textColor="@color/location"
            app:layout_constraintTop_toBottomOf="@+id/fullname"
            app:layout_constraintStart_toStartOf="@+id/fullname"
            android:textSize="12sp"/>

    <ImageView
            android:layout_width="@dimen/image_rw_land"
            android:layout_height="@dimen/image_rw_land"
            app:srcCompat="@drawable/rwlogo"
            android:id="@+id/imageView14"
            app:layout_constraintStart_toStartOf="@+id/imageView13"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView13"
            android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp"/>

    <TextView
            android:text="@string/email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11"
            app:fontFamily="@font/contentfont"
            app:layout_constraintTop_toTopOf="@+id/imageView12"
            app:layout_constraintBottom_toBottomOf="@+id/imageView12"
            app:layout_constraintStart_toStartOf="@+id/textView10"/>

    <ImageView
            android:layout_width="@dimen/image_size_horizontal"
            android:layout_height="@dimen/image_size_horizontal"
            app:srcCompat="@drawable/logounam"
            android:id="@+id/imageView13"
            app:layout_constraintTop_toTopOf="@+id/imageView11"
            app:layout_constraintEnd_toStartOf="@+id/textView12"
            android:layout_marginEnd="32dp"
            android:layout_marginRight="32dp"/>

    <TextView
            android:text="@string/occupation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12"
            app:fontFamily="@font/contentfont"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="92dp"
            android:layout_marginRight="92dp"
            app:layout_constraintStart_toStartOf="@+id/textView13"
            app:layout_constraintTop_toTopOf="@+id/imageView13"
            app:layout_constraintBottom_toBottomOf="@+id/imageView13"/>

    <TextView
            android:text="@string/occupation2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView13"
            app:fontFamily="@font/contentfont"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            app:layout_constraintTop_toTopOf="@+id/imageView14"
            app:layout_constraintBottom_toBottomOf="@+id/imageView14"/>

    <ImageView
            android:layout_width="@dimen/image_size_horizontal"
            android:layout_height="@dimen/image_size_horizontal"
            app:srcCompat="@drawable/twitter"
            android:id="@+id/iv_twitter"
            app:layout_constraintStart_toEndOf="@+id/fullname"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/iv_github"
            app:layout_constraintTop_toTopOf="@+id/iv_github"/>

    <ImageView
            android:layout_width="@dimen/image_size_horizontal"
            android:layout_height="@dimen/image_size_horizontal"
            app:srcCompat="@drawable/linkedin"
            android:id="@+id/iv_linkedin"
            app:layout_constraintStart_toEndOf="@+id/iv_github"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/iv_github"/>

    <ImageView
            android:layout_width="@dimen/image_size_horizontal"
            android:layout_height="@dimen/image_size_horizontal"
            app:srcCompat="@drawable/github"
            android:id="@+id/iv_github"
            app:layout_constraintStart_toEndOf="@+id/iv_twitter"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/iv_linkedin"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"/>

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="@dimen/section_width_ldetail"
            android:layout_height="@dimen/section_height_ldetail"
            android:background="@drawable/rounded_card"
            app:layout_constraintTop_toBottomOf="@+id/iv_github"
            android:layout_marginTop="32dp"
            android:id="@+id/linearLayout4"
            app:layout_constraintStart_toStartOf="@+id/imageView13">

        <TextView
                android:text="@string/string_occupation"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewjj"
                android:layout_weight="1"
                android:textColor="@color/white"
                app:fontFamily="@font/sections"
                android:textSize="@dimen/sections_textsize_land"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/text_section_start"
                android:layout_marginLeft="@dimen/text_section_start"/>

    </LinearLayout>


    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="@dimen/section_width_ldetail"
            android:layout_height="@dimen/section_height_ldetail"
            android:background="@drawable/rounded_card"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_twitter"
            android:id="@+id/linearLayout3"
            app:layout_constraintStart_toStartOf="@+id/fullname">

        <TextView
                android:text="@string/string_contact"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/white"
                app:fontFamily="@font/sections"
                android:textSize="@dimen/sections_textsize_land"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/text_section_start"
                android:layout_marginLeft="@dimen/text_section_start"/>
    </LinearLayout>

    <ImageView
            android:layout_width="@dimen/image_size_horizontal"
            android:layout_height="@dimen/image_size_horizontal"
            app:srcCompat="@drawable/telegram"
            android:id="@+id/imageView11"
            android:layout_weight="1"
            app:layout_constraintStart_toStartOf="@+id/linearLayout3"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3"/>

    <com.google.android.material.button.MaterialButton
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button"
            style="@style/Widget.MaterialComponents.Button.OutlinedButton"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/button_margin"
            android:layout_marginRight="@dimen/button_margin"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="@dimen/button_margin"
            android:layout_marginStart="@dimen/button_margin"
            app:strokeColor="@color/colorPrimary"
            android:textAppearance="@style/TextAppearance.MaterialComponents.Button"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="32dp"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView14"/>

</androidx.constraintlayout.widget.ConstraintLayout>